{
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "mongoose": "^6.5.4",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "v5.0.0-alpha.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "test": "jest --coverage"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.13",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.3.0",
    "jest": "^29.0.1",
    "mongodb-memory-server": "^8.9.0",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
